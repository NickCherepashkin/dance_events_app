<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/events"
    app:startDestination="@id/eventsListFragment">

    <fragment
        android:id="@+id/eventsListFragment"
        android:name="com.drozdova.danceevents.presentation.EventsListFragment"
        android:label="fragment_events_list"
        tools:layout="@layout/fragment_events_list">
        <action
            android:id="@+id/action_eventsListFragment_to_eventInfoFragment2"
            app:destination="@id/eventInfoFragment2"
            />
    </fragment>
    <fragment
        android:id="@+id/eventInfoFragment2"
        android:name="com.drozdova.danceevents.presentation.EventInfoFragment"
        android:label="fragment_event_info"
        tools:layout="@layout/fragment_event_info" />
</navigation>